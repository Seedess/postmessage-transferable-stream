{
  "compilerOptions": {
      "outDir": "./public/dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "esModuleInterop": true,
      "allowJs": true,
      "lib": ["es5", "dom"],
      "types": ["cypress", "node"],
      "downlevelIteration": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
}